{
    "buildCommand": "npm run build",
    "framework": "vite",
    "build": {
        "env": {
            "VITE_FORCE_REBUILD": "true"
        }
    },
    "rewrites": [
        {
            "source": "/api/n8n/:path*",
            "destination": "https://n8n.neuracall.net/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}